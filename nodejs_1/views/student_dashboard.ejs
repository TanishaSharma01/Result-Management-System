<!-- include header -->
<%- include('include/_header') %>
<!-- /include header -->

<main id="site-main">
    <div class="container">
        <div class="box-nav d-flex justify-between">
            <div class="filter">
                <a href="/"><i class="fas fa-angle-double-left"></i>Home</a>
            </div>
        </div>
        <div class="form-title text-center">
            <h2 class="text-dark">Your Result</h2>
        </div>
        <form>
            <table class="table">
                <thead class="thead-dark">
                    <tr>
                        <th>Roll No.</th>
                        <th>Name</th>
                        <th>Date of Birth</th>
                        <th>Score</th>
                    </tr>
                </thead>
                <tbody>
                    <tr>
                        <td><%= one.rollno %> </td>
                        <td><%= one.name %> </td>
                        <td><%= one.dob %> </td>
                        <td><%= one.score %> </td>
                      </tr>
                </tbody>
            </table>
        </form>
    </div>
</main>

<!--include footer -->
<%- include('include/_footer') %>