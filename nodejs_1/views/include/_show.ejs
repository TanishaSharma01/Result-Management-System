<% for(var i = 0; i < results.length; i++) { %>
    <tr>
        <td><%= results[i].rollno %></td>
        <td><%= results[i].name %></td>
        <td><%= results[i].dob %></td>
        <td><%= results[i].score %></td>
        <td>
            <a href="/update-result?id=<%= results[i]._id %>" class="btn border-shadow update">
                <span class="text-gradient"><i class="fas fa-pencil-alt"></i></span>
            </a>
            <a class="btn border-shadow delete" data-id=<%= results[i]._id %>>
                <span class="text-gradient"><i class="fa-solid fa-trash"></i></span>
            </a>
        </td>
    </tr>
    <% } %>